<?xml version="1.0"?>
<definitions xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/" 
xmlns:tns="http://localhost:2000/wsdl/hws.wsdl"
xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/" 
xmlns:si="http://soapinterop.org/xsd" 
xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
xmlns="http://schemas.xmlsoap.org/wsdl/" 
targetNamespace="http://localhost:2000/wsdl/hws.wsdl">
<message name="getInvoices">
   <part name="from" type="xsd:date"/>
   <part name="to" type="xsd:date"/>
</message>

<message name="getInvoiceResponse">
   <part name="queryInvoiceResponse" type="xsd:string"/>
</message>

<portType name="queryPort">
  <operation name="queryInvoice">
      <input message="getInvoices"/>
      <output message="getInvoiceResponse"/>
  </operation>
</portType>
<binding name="queryBinding" type="tns:queryPort">
   <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
   <operation name="queryInvoice">
       <soap:operation soapaction="http://localhost:2000/wsdl/hws.wsdl#queryInvoice" style="rpc"/>
      <input>
         <soap:body use="encoded" namespace="http://localhost:2000/wsdl/hws.wsdl" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </input>
      <output>
         <soap:body use="encoded" namespace="http://localhost:2000/wsdl/hws.wsdl" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </output>
   </operation>
 </binding>
 <service name="hws">
    <documentation>The service returns subscriptions paid by invoice in the period from(date)
    to to(date)</documentation>
    <port name="queryPort" binding="tns:queryBinding">
       <soap:address location="http://localhost:2000/"/>
    </port>
</service>
</definitions>